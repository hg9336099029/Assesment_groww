# Finance Dashboard - Environment Variables
# Copy this file to .env and fill in your API keys

# Alpha Vantage API Key
# Get your free API key at: https://www.alphavantage.co/support/#api-key
VITE_ALPHA_VANTAGE_API_KEY=your_alpha_vantage_key_here

# Finnhub API Key
# Get your free API key at: https://finnhub.io/register
VITE_FINNHUB_API_KEY=your_finnhub_key_here

# IndianAPI Key (if using)
VITE_INDIAN_API_KEY=your_indian_api_key_here

# Cache duration in seconds (default: 30)
VITE_CACHE_DURATION=30

# Default refresh interval in seconds (default: 60)
VITE_DEFAULT_REFRESH_INTERVAL=60
